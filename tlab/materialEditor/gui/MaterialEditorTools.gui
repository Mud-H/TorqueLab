$MaterialEditorTools_SwatchButtons = "colorTintSwatch specularColorSwatch subSurfaceColorSwatch effectColor0Swatch effectColor1Swatch";
//==============================================================================
function MaterialEditorTools::onPreEditorSave( %this ) {
   foreach$(%ctrlInt in $MaterialEditorTools_SwatchButtons){
      %guiCtrl = %this.findObjectByInternalName(%ctrlInt, true);
       if (!isObject(%guiCtrl))
         continue;
     %guiCtrl.gridBitmap = "";
      LabObj.removeField(%guiCtrl,"gridBitmap"); 
      devLog(%ctrlInt,"after",%guiCtrl.gridBitmap);
	
   }
   LabObj.saveAll();
}
